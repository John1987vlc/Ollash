{
  "sandbox": "limited",
  "max_context_tokens": 16384,
  "summarize_threshold_ratio": 0.7,
  "history_limit": 20,
  "log_level": "debug",
  "log_file": "ollash.log",
  "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
  "default_system_prompt_path": "prompts/orchestrator/default_orchestrator.json",
  "use_docker_sandbox": false,
  "rate_limiting": {
    "requests_per_minute": 300,
    "max_tokens_per_minute": 2000000
  },
  "gpu_rate_limiter": {
    "enabled": false,
    "degradation_threshold_ms": 20000.0,
    "recovery_threshold_ms": 5000.0,
    "min_rpm": 60,
    "ema_alpha": 0.2
  },
  "auto_confirm_tools": false,
  "max_iterations": 30,
  "max_depth": 2,
  "loop_detection_threshold": 3,
  "semantic_similarity_threshold": 0.95,
  "parallel_generation_max_concurrent": 3,
  "parallel_generation_max_rpm": 10,
  "senior_review_max_attempts": 3,
  "completeness_checker_max_retries": 2,
  "token_encoding_name": "cl100k_base"
}
